{
  "name": "chemi_manage",
  "description": "a chemical management system",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build:tailwind": "tailwind build -i src/tailwind.css -o src/tailwind.output.css",
    "watch:tailwind": "npm run build:tailwind -- --watch",
    "watch": "npm-run-all build:tailwind --parallel watch:tailwind dev",
    "prebuild": "run-s build:tailwind",
    "dev": "vite",
    "start": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "serve": "vite preview --host 0.0.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.0.0-rc.0",
    "@emotion/styled": "^11.8.1",
    "query-string": "^7.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
